PARSER_BEGIN(Robot)

package uniandes.lym.robot.control;

import uniandes.lym.robot.kernel.*;
import uniandes.lym.robot.view.Console;

import java.awt.Point;
import java.io.*;
import java.util.*;

@SuppressWarnings("serial")
public class Robot 
{
	private RobotWorldDec world;


	void setWorld(RobotWorld w) {
		world = (RobotWorldDec) w;	
	}


	private String salida = new String();

	private ArrayList<String> declaredVars = new ArrayList<String>();
	private Hashtable<String, Integer> vars = new Hashtable<String, Integer>();
	private ArrayList<String> procsNames = new ArrayList<String>();
	private Hashtable<String, ArrayList<String>> procsParams = new Hashtable<String, ArrayList<String>>();
	private Hashtable<String, ArrayList<String>> procsInstrs = new Hashtable<String, ArrayList<String>>();
	private Hashtable<String, String> currentParams = new Hashtable<String, String>();
}

PARSER_END(Robot)


SKIP:
{
		" "
	|	"\r"
	|	"\t"
	|	"\n"
}


/* Commands */
TOKEN: 
{
		< STEP: "step" >
	|	< JUMP: "jump" >
	|	< JUMPTO: "jumpTo" >
	|	< VEER: "veer" >
	|	< LOOK: "look" >
	|	< DROP: "drop" >
	|	< GRAB: "grab" >
	|	< GET: "get" >
	|	< FREE: "free" >
	|	< POP: "pop" >
	| 	< DMOVE:  "Dmove" >
	|	< OMOVE:  "Omove" >
}


/* Control structures */
TOKEN :
{
		< IF: "if" >
	|	< FI: "fi" >
	|	< ELSE: "else" >
	| 	< WHILE: "while" >
	|	< DO: "do" >
	| 	< OD: "od" >
	|	< REPEATTIMES: "repeatTimes" >
	|	< PER : "per" >
}


/* Conditions */
TOKEN :
{
		< ISFACING : "isfacing" >
	| 	< ISVALID: "isValid" >
	| 	< CANMOVE: "canMove" >
	| 	< NOT: "not" >
	|	< PICK:  "pick" > // For isValid condition
}


/* Cardinal directions */
TOKEN :
{
	 	< NORTH: "north" >
	| 	< SOUTH: "south" >
	| 	< EAST: "east" >
	| 	< WEST: "west" >
}


/* Directions */
TOKEN :
{
		< AROUND: "around" >
	| 	< RIGHT: "right" >
	| 	< LEFT: "left" >
	| 	< FRONT: "front" >
	| 	< BACK: "back" >
}


/* Keywords */
TOKEN :
{
		< PROG: "PROG" >
	|	< GORP: "GORP" >
	|	< VAR: "VAR" >
	|	< PROC: "PROC" >
	|	< CORP: "CORP" >
}


/* Name */
TOKEN :
{
		< NAME: <LETTER>(["a"-"z","A"-"Z","0"-"9"])* >
	|  	< #LETTER: ["a"-"z","A"-"Z"] >
}

/* Number */
TOKEN :
{
		< NUM: (<DIGIT>)+ >
	|  	< #DIGIT: ["0"-"9"] >
}


	boolean command(Console sistema):
	{
		this.salida = "";
		this.declaredVars = new ArrayList<String>();
	 	this.vars = new Hashtable<String, Integer>();
		this.procsNames = new ArrayList<String>();
		this.procsParams = new Hashtable<String, ArrayList<String>>();
		this.procsInstrs = new Hashtable<String, ArrayList<String>>();
		this.currentParams = new Hashtable<String, String>();
	}
	{
		( < PROG > [varDecl()] (procDef())* instrBlock() < GORP >) {
		    try {
	    		Thread.sleep(900);
	    	} catch (InterruptedException e) {
	    		System.err.format("IOException: %s%n", e);
	    	}
			sistema.printOutput(salida);
			return true;
		}
    	| <EOF> {return false;}
	}


	void varDecl():
	{
		String name;
	}
	{
		((< VAR > name=name()) {
			this.declaredVars.add(name);
		} ( LOOKAHEAD(2) "," (name=name()) {
			if (this.declaredVars.contains(name)) {
				throw new Error("Repeated variable name: "+name+ " in variable declaration");
			} else {
				this.declaredVars.add(name);
			}
		} )* ";" ) {
			this.salida = this.salida + "\nVariable declaration";
		}
	}


	void procDef():
	{
		String name;
		ArrayList<String> localVars = new ArrayList<String>();
		ArrayList<String> instrs = new ArrayList<String>();
		ArrayList<String> l = new ArrayList<String>();
		this.currentParams = new Hashtable<String, String>();
	}
	{
		 (( < PROC > name=name() "(" [localVars=params()] ")"
		 ) {
			if (procsNames.contains(name)) {
				throw new Error ("Procedure with name "+name+" has already been declared");
			}
			procsNames.add(name);
			procsParams.put(name, localVars);
		} (  "{" l = saveInstr() { instrs.addAll(l); } (  ";" l = saveInstr() { instrs.addAll(l); } )* "}" < CORP > ))
		{	this.procsInstrs.put(name, instrs); 
			this.salida = this.salida + "\nProcedure definition";
			this.currentParams = new Hashtable<String, String>();}
	}


	ArrayList<String> params():
	{
		ArrayList<String> localVars = new ArrayList<String>();
		String name;
	}
	{
		((name=name()) {
			localVars.add(name);
			this.currentParams.put(name, name);
		} ( LOOKAHEAD(2) "," (name=name()) {
			if (localVars.contains(name)) {
				throw new Error("Repeated parameter name: "+name);
			} else {
				localVars.add(name);
				this.currentParams.put(name, name);
			}
		} )*) { return localVars; }
	}


	void instrBlock():
	{
	}
	{
		"{" instr() ( LOOKAHEAD(2) ";" instr() )* "}"
	}


	void instr():
	{
	}
	{
		LOOKAHEAD(2) cmd()
	|	ctrlStruct()
	|	procCall()
	}


	void cmd():
	{
		String name;
		Integer n, m;
	}
	{
		((name=name() ":=" n = num()) {
			if (declaredVars.contains(name)) {
				vars.put(name, n);
			} else {
				throw new Error("Tried to assign to undeclared variable "+name);
			}
			this.salida = this.salida + "\nCommand: Variable assignment";
		})
	|	(< STEP >  "(" n = numVarParam() ")" {world.moveForward(n, false); this.salida = this.salida + "\nCommand: Move steps forward ";})
	|	(< JUMP >  "(" n  = numVarParam() ")" {world.moveForward(n, true); this.salida = this.salida + "\nCommand: Jump steps forward ";})
	|	(< JUMPTO >  "(" n  = numVarParam() "," m = numVarParam()")" {world.setPostion(n,m); this.salida = this.salida + "\nCommand: Jump to position ";})
	|	(< VEER >  "(" veer() ")")
	|	(< LOOK >  "(" look() ")")
	|	(< DROP >  "(" n  = numVarParam() ")" {world.putChips(n); this.salida = this.salida + "\nCommand: Drop chips from its position ";})
	|	(< GRAB >  "(" n  = numVarParam() ")" {world.grabBalloons(n); this.salida = this.salida + "\nCommand: Grab balloons from its position ";})
	|	(< GET >  "(" n  = numVarParam() ")" {world.pickChips(n); this.salida = this.salida + "\nCommand: Get chips from its position ";})
	|	(< FREE >  "(" n  = numVarParam() ")" {world.putBalloons(n); this.salida = this.salida + "\nCommand: Put balloons from its position ";})
	|	(< POP >  "(" n  = numVarParam() ")" {world.popBalloons(n); this.salida = this.salida + "\nCommand: Pop balloons from its position ";})
	|	(< DMOVE >  "(" dMove() ")")
	|	(< OMOVE >  "(" oMove() ")")
	}


	void ctrlStruct():
	{
	}
	{
		ifFi()
	|	whileDoOd()
	|	repeatTimes()
	}


	void ifFi():
	{
		boolean bool;
	}
	{
		(< IF > "(" bool=condition() ")"
			"{" instrIf(bool) ( ";" instrIf(bool) )* "}"
		[< ELSE > 
			"{" instrIf(!bool) (  ";" instrIf(!bool) )* "}"
		] < FI >) {
		this.salida = this.salida + "\n'if' conditional control structure";
		}
	}


JAVACODE
	private void instrIf(boolean bool) {
		if (bool) {
			instr();
		} else {
			checkSintaxInstr();
		}
	}


	void whileDoOd():
	{
		ArrayList<String> cond = new ArrayList<String>();
	}
	{
		< WHILE > "(" cond = saveCond() ")" < DO > whileInstrBlock(cond) < OD > {
			this.salida = this.salida + "\n'while' loop control structure";
			}
	}


	void repeatTimes():
	{
		Integer n;
	}
	{
		( < REPEATTIMES > n = numVarParam() ) repeatInstrBlock(n) ( < PER > ) {
			this.salida = this.salida + "\n'repeatTimes' loop control structure";
		}
	}


	boolean condition():
	{
		boolean bool;
	}
	{
		(< ISFACING >  "(" bool=isFacing() ")"
	|	< ISVALID >  "(" bool=isValid() ")"
	|	< CANMOVE >  "(" bool=canMove() ")"
	|	< NOT >  "(" bool=not() ")") {return bool;}
	}


	void procCall():
	{
		String name;
		ArrayList<Integer> numsParams = new ArrayList<Integer>();
	}
	{
		name = name() "(" [numsParams = numParams()] ")" {
			if (!procsNames.contains(name)) {
				throw new Error("Tried to call undeclared procedure");
			}
			if (numsParams.size() != procsParams.get(name).size()) {
				throw new Error("Incorrect number of parameters for "+name+" procedure");
			}
			callProcedure(name, numsParams);
		}
	}


	ArrayList<Integer> numParams():
	{
		ArrayList<Integer> nums = new ArrayList<Integer>();
		Integer n;
	}
	{
		n = numVarParam() {nums.add(n);} ( "," n = numVarParam() {nums.add(n);} )* { return nums; }
	}


JAVACODE
	private void callProcedure(String name, ArrayList<Integer> nums) {
		ArrayList<String> params = this.procsParams.get(name);
		this.currentParams = new Hashtable<String, String>();
		
		for (Integer i = 0; i < params.size(); i++) {
			this.currentParams.put(params.get(i), Integer.toString(nums.get(i)));
		}
		ArrayList<String> instrs = this.procsInstrs.get(name);
		manuallyExecuteInstrs(instrs);
		this.currentParams = new Hashtable<String, String>();
	}


